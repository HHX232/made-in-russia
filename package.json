{
  "name": "made-in-china",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "tsc && next build",
    "deploy:static": "npm run build && node deploy-ftp.cjs",
    "deploy:static:only": "node deploy-ftp.cjs",
    "start": "next start",
    "cypress:open": "cypress open",
    "test": "jest",
    "test:watch": "jest --watch",
